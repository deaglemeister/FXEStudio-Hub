                <variant value="dd.MM.yyyy">[21.03.2015]</variant>
                <variant value="yyyy.MM.dd">[2015.03.21]</variant>
                <variant value="dd.MM.yy">[21.03.15]</variant>

                <variant value="dd/MM/yyyy">[21/03/2015]</variant>
