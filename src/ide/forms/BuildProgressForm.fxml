<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="431.0" prefWidth="603.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox id="content" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox id="header" alignment="CENTER_LEFT" spacing="15.0">
               <children>
                  <ImageView id="icon" fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true" />
                  <VBox>
                     <children>
                        <Label id="workTitle" prefHeight="21.0" prefWidth="498.0" text="Сборка проекта">
                           <font>
                              <Font name="Tahoma" size="17.0" />
                           </font>
                        </Label>
                        <Label id="workDescription" prefHeight="15.0" prefWidth="526.0" text="Подождите пару мгновений ..." textFill="#7c7c7c">
                           <font>
                              <Font name="Tahoma" size="12.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <ListView id="consoleList" styleClass="dn-console-list" VBox.vgrow="ALWAYS" />
            <ProgressBar id="progress" maxWidth="1.7976931348623157E308" minHeight="-Infinity" prefHeight="23.0" />
            <HBox alignment="CENTER_LEFT" id="bottomPane" minHeight="-Infinity" prefHeight="35.0" spacing="15.0">
               <children>
                  <Button id="closeButton" mnemonicParsing="false" prefHeight="35.0" text="Закрыть">
                     <cursor>
                        <Cursor fx:constant="DEFAULT" />
                     </cursor>
                     <padding>
                        <Insets left="15.0" right="15.0" />
                     </padding>
                     <styleClass>
                        <String fx:value="icon-square" />
                        <String fx:value="foobar" />
                     </styleClass>
                  </Button>
                  <CheckBox id="closeAfterDoneCheckbox" mnemonicParsing="false" selected="true" text="Закрыть после выполнения" />
                  <Separator orientation="VERTICAL">
                  </Separator>
                  <Hyperlink id="message" textFill="red" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
